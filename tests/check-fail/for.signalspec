@test(#up, ())
with Seq(#x, _) def main() = {
//# expected a single value
    for x = () {
//          ^^ found `()`

    }
}


@test(#up, ())
with Seq(#x, _) def main() = {
//# expected vector
    for x = #a {
//          ^^ found `#a`

    }
}

@test(#up, ())
with Seq(#x, _) def main() = {
//# `for` loop vectors must be the same width
    for x = [#a, #b], y = [#x], z = [#t, #u] {
//          ^^^^^^^^ vector of width 2
//                        ^^^^ vector of width 1
    }
}

