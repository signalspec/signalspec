protocol Packets(t, dir) dir {
    packet() : Seq(t, dir)
}

with Seq(_:#a|#end, dir)
def pkt() = new Packets(_:#a, dir) {
    repeat {
        on packet() {
            repeat {
                on val(#a) {
                    val(#a)
                }
            }
        }
        val(#end)
    }
}

@xtest(#both, (#a, #a, #end, #end, #a, #end))
with Seq(_:#a|#end, _)
def t1() = pkt() | seq {
    packet() | seq {
        val(#a)
        val(#a)
    }
    packet() | seq {}
    packet() | val(#a)
}
