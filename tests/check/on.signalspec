with Seq(0..10)
def main() = Seq(0..10) {
    on val(1) {}
    on val(2) {}
    on val(a) {
        val(a)
        val(a)
    }
}

test @both(Seq(0..10)) { val(5); val(5) } | main() | { val(1) ; val(2) ; val(5) };

with Seq((0..10, 0..10))
def tuple() = Seq((0..10, 0..10)) {
    on val(a, b) {
        val(a, b)
        val(a, b)
    }

    on val(x) {
        val(x)
        val(x)
    }
}

test @both(Seq((0..10, 0..10))) { val(5, 6); val(5, 6); val(7, 8); val(7, 8) } | tuple() | { val(5, 6); val(7, 8) };
