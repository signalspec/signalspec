with Seq(#0..#10)
def main(): Seq(#0..#10) {
    on #1 {}
    on #2 {}
    on a {
        a
        a
    }
}

test @both(Seq(#0..#10)) { #5; #5 } main() { #1 ; #2 ; #5 };

with Seq((#0..#10, #0..#10))
def tuple(): Seq((#0..#10, #0..#10)) {
    on (a, b) {
        (a, b)
        (a, b)
    }

    on x {
        x
        x
    }
}

test @both(Seq((#0..#10, #0..#10))) { (#5, #6); (#5, #6); (#7, #8); (#7, #8) } tuple() { (#5, #6); (#7, #8) };
