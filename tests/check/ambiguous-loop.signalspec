with Seq(#x|#y, _)
def main2() = {
    val(#x)
    repeat _ { val(#x) }
}

test @up(Seq(#x|#y, _)) { val(#x); val(#x); val(#x); val(#x) } | main2();
test fail @up(Seq(#x|#y, _)) { val(#y); val(#x); val(#x); val(#x) } | main2();

with Seq(#x|#y, _)
def main3() = {
    val(#x)
    repeat 3 { val(#x) }
    val(#y)
}

test @up(Seq(#x|#y, _)) { val(#x); val(#x); val(#x); val(#x); val(#y) } | main3();
test fail @up(Seq(#x|#y, _)) { val(#x); val(#x); val(#x); val(#y) } | main3();
