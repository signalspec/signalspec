let t = #a|#b|#c|#d

with Seq(t)
def main(x) {
    #a
    #b
    repeat x {
        #c
        #d
    }
    #a
}

test @both(Seq(t)) { #a ; #b ; #a } main(#0);
test fail @up(Seq(t)) { #a ; #b ; #a } main(#1);
test @both(Seq(t)) { #a ; #b ; #c ; #d ; #a } main(#1);
test @both(Seq(t)) { #a ; #b ; #c ; #d ; #c ; #d ; #a } main(#2);
test fail @up(Seq(t)) { #a ; #b ; #c ; #a } main(_);
test fail @up(Seq(t)) { #a ; #b ; #c ; #d } main(_);
