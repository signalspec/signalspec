with Seq(#a|#b, _) def main() = {
    inner() | {
        val(#x)
        val(#y)
        val(#y)
    }
}

with Seq(#a|#b, dir) def inner() = Seq(#x|#y, dir) {
    repeat {
        on val(i) {
            val(i[#x=#a, #y=#b])
        }
    }
}

test @both(Seq(#a|#b, _)) { val(#a); val(#b); val(#b) } | main();
