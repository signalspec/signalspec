let t = #a|#b|#c|#d|#e

with Seq(t)
def main() {
    #a
    repeat {
        repeat {
            #b
        }
        repeat {
            #c
        }
        #d
    }
    #e
}

test @up(Seq(t)) { #a ; #e } main();
test @up(Seq(t)) { #a ; #b ; #c ; #d ; #e } main();
test @up(Seq(t)) { #a ; #b ; #b ; #c ; #c ; #c ; #d ; #b ; #d ; #e } main();
test fail @up(Seq(t)){ #a ; #b ; #c } main();
